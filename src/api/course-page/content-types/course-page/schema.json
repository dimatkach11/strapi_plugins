{
  "kind": "collectionType",
  "collectionName": "course_pages",
  "info": {
    "singularName": "course-page",
    "pluralName": "course-pages",
    "displayName": "Course Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "content": {
      "type": "blocks"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "free_course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::free-course.free-course"
    },
    "paid_course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::paid-course.paid-course"
    },
    "course_options": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-option.course-option",
      "inversedBy": "course_pages"
    },
    "c_course_types": {
      "type": "customField",
      "options": {
        "enum": [
          "free_course",
          "paid_course"
        ]
      },
      "customField": "plugin::parent-child-relationships.custom-enumeration"
    },
    "c_course": {
      "type": "customField",
      "options": {
        "name": "c_course_types",
        "parent": "c_course_types",
        "common_relational_table": "course"
      },
      "customField": "plugin::parent-child-relationships.parent-child-relation"
    },
    "c_course_options": {
      "type": "customField",
      "options": {
        "name": "course_options",
        "parent": "c_course",
        "common_relational_table": ""
      },
      "customField": "plugin::parent-child-relationships.parent-child-relation"
    }
  }
}
