{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "brand": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::brand.brand"
    },
    "model": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::model.model"
    },
    "version": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::version.version"
    },
    "c_brand": {
      "type": "customField",
      "options": {
        "name": "brand",
        "current_column": "code",
        "child_column": "brand_code"
      },
      "customField": "plugin::parent-child-relationships.relation"
    },
    "c_model": {
      "type": "customField",
      "options": {
        "name": "model",
        "current_column": "model_code",
        "child_column": "version_code",
        "parent": "c_brand",
        "params": {
          "db_columns": [
            "model_code: different_model_code_name"
          ],
          "statics": [
            "static_value: generic_param_name"
          ]
        }
      },
      "customField": "plugin::parent-child-relationships.relation"
    },
    "c_version": {
      "type": "customField",
      "options": {
        "name": "version",
        "parent": "c_model",
        "params": {
          "db_columns": [
            ""
          ]
        },
        "filter": "{\"segment\": \"sport\", \"name\": {\"$containsi\": \"xdrive\"}}"
      },
      "customField": "plugin::parent-child-relationships.relation"
    }
  }
}
